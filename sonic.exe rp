-- Scripts:

local function GYZPVQQ_fake_script() -- SansEye.Script 

	local script = Instance.new('Script', SansEye)

	function onclick()

		local name = game:GetService("Players").LocalPlayer.Name

		print(name)

	

		game.workspace[name].head.pupil_1:Destroy()

		--be.exe

		wait(4)

		--be normal lol

		game.Workspace[name].head.iris_1:Destroy()

	end

	

	script.Parent.MouseButton1Click:connect(onclick)

	

end

coroutine.wrap(GYZPVQQ_fake_script)()

