local function YIMNVS_fake_script() -- BehindSallyexeseyes.Script 

	local script = Instance.new('Script', BehindSallyexeseyes)

	function onclick()

		local name = game:GetService("Players").LocalPlayer.Name

		game.workspace[name].head.i_eyes:Destroy()

		wait(0.3)

		game.workspace[name].head.i_eyes_blood:Destroy()

	end

end

coroutine.wrap(YIMNVS_fake_script)()

local function SSJVVV_fake_script() -- Page1.LocalScript 

	local script = Instance.new('LocalScript', Page1)

	--Not made by me, check out this video: https://www.youtube.com/watch?v=z25nyNBG7Js&t=22s

	--Put this inside of your Frame and configure the speed if you would like.

	--Enjoy! Credits go to: https://www.youtube.com/watch?v=z25nyNBG7Js&t=22s

	

	local UIS = game:GetService('UserInputService')

	local frame = script.Parent

	local dragToggle = nil

	local dragSpeed = 0.25

	local dragStart = nil

	local startPos = nil

	

	local function updateInput(input)

		local delta = input.Position - dragStart

		local position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X,

			startPos.Y.Scale, startPos.Y.Offset + delta.Y)

		game:GetService('TweenService'):Create(frame, TweenInfo.new(dragSpeed), {Position = position}):Play()

	end

	

	frame.InputBegan:Connect(function(input)

		if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) then 

			dragToggle = true

			dragStart = input.Position

			startPos = frame.Position

			input.Changed:Connect(function()

				if input.UserInputState == Enum.UserInputState.End then

					dragToggle = false

				end

			end)

		end

	end)

	

	UIS.InputChanged:Connect(function(input)

		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then

			if dragToggle then

				updateInput(input)

			end

		end

	end)

	

end

coroutine.wrap(SSJVVV_fake_script)()

local function URADRK_fake_script() -- Next.Script 

	local script = Instance.new('Script', Next)

	function onclick()

		local page1 = script.Parent.Parent.Parent.Page1

		local pages = script.Parent.Pages

		local page2 = script.Parent.Parent.Parent.Page2

		if page1.Visible == true then

			

		

	script.Parent.Visible = false

			page1.Visible = true

			page2.Visible = true

			pages.Text = "Pages = 2/2"

			script.Parent.Visible = false

			script.Parent.Parent.Parent.Page2.Next.Visible = true

		end

		end

	script.Parent.MouseButton1Click:connect(onclick)

	

end

coroutine.wrap(URADRK_fake_script)()

local function WXGNECK_fake_script() -- Close.LocalScript 

	local script = Instance.new('LocalScript', Close)

	script.Parent.MouseButton1Click:Connect(function()

		script.Parent.Parent.Parent:Destroy()

	end)

	

end

coroutine.wrap(WXGNECK_fake_script)()

local function PIZCR_fake_script() -- TRUEEGG.RainbowText 

	local script = Instance.new('Script', TRUEEGG)

	local text = script.Parent

	local add = 10

	wait(1)

	local k = 1

	while k <= 255 do

	text.TextColor3 = Color3.new(k/255,0/255,0/255)

	k = k + add

	wait()

	end

	while true do

	k = 1

	while k <= 255 do

	text.TextColor3 = Color3.new(255/255,k/255,0/255)

	k = k + add

	wait()

	end

	k = 1

	while k <= 255 do

	text.TextColor3 = Color3.new(255/255 - k/255,255/255,0/255)

	k = k + add

	wait()

	end

	k = 1

	while k <= 255 do

	text.TextColor3 = Color3.new(0/255,255/255,k/255)

	k = k + add

	wait()

	end

	k = 1

	while k <= 255 do

	text.TextColor3 = Color3.new(0/255,255/255 - k/255,255/255)

	k = k + add

	wait()

	end

	k = 1

	while k <= 255 do

	text.TextColor3 = Color3.new(k/255,0/255,255/255)

	k = k + add

	wait()

	end

	k = 1

	while k <= 255 do

	text.TextColor3 = Color3.new(255/255,0/255,255/255 - k/255)

	k = k + add

	wait()

	end

	while k <= 255 do

	text.TextColor3 = Color3.new(255/255 - k/255,0/255,0/255)

	k = k + add

	wait()

	end

	end

end

coroutine.wrap(PIZCR_fake_script)()

local function IHKNXK_fake_script() -- TRUEEGG.Script 

	local script = Instance.new('Script', TRUEEGG)

	function onclick()

		local name = game:GetService("Players").LocalPlayer.Name

		print(name)

		game.Workspace[name].head:Destroy()

		game.Workspace[name].arm1:Destroy()

		game.Workspace[name].arm2:Destroy()

		game.Workspace[name].leg1:Destroy()

		game.Workspace[name].leg2:Destroy()

		game.Workspace[name].Humanoid.HipHeight = 1

	end

	

	script.Parent.MouseButton1Click:connect(onclick)

	

end

coroutine.wrap(IHKNXK_fake_script)()

local function FVABP_fake_script() -- Page2.LocalScript 

	local script = Instance.new('LocalScript', Page2)

	--Not made by me, check out this video: https://www.youtube.com/watch?v=z25nyNBG7Js&t=22s

	--Put this inside of your Frame and configure the speed if you would like.

	--Enjoy! Credits go to: https://www.youtube.com/watch?v=z25nyNBG7Js&t=22s

	

	local UIS = game:GetService('UserInputService')

	local frame = script.Parent

	local dragToggle = nil

	local dragSpeed = 0.25

	local dragStart = nil

	local startPos = nil

	

	local function updateInput(input)

		local delta = input.Position - dragStart

		local position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X,

			startPos.Y.Scale, startPos.Y.Offset + delta.Y)

		game:GetService('TweenService'):Create(frame, TweenInfo.new(dragSpeed), {Position = position}):Play()

	end

	

	frame.InputBegan:Connect(function(input)

		if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) then 

			dragToggle = true

			dragStart = input.Position

			startPos = frame.Position

			input.Changed:Connect(function()

				if input.UserInputState == Enum.UserInputState.End then

					dragToggle = false

				end

			end)

		end

	end)

	

	UIS.InputChanged:Connect(function(input)

		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then

			if dragToggle then

				updateInput(input)

			end

		end

	end)

	

end

coroutine.wrap(FVABP_fake_script)()

local function NAIQVRT_fake_script() -- Next_2.Script 

	local script = Instance.new('Script', Next_2)

	function onclick()

		local page1 = script.Parent.Parent.Parent.Page1

		local pages = script.Parent.Pages

		local page2 = script.Parent.Parent.Parent.Page2

		if page2.Visible == true then

	

	

	

			

			page1.Visible = true

			page2.Visible = false

			pages.Text = "Pages = 1/2"

			script.Parent.Text = "Back"

			script.Parent.Visible = false

			script.Parent.Parent.Parent.Page1.Next.Visible = true

		end

	

	end

	

	script.Parent.MouseButton1Click:connect(onclick)

	

end

coroutine.wrap(NAIQVRT_fake_script)()
