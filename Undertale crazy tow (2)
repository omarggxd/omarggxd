--Function
_G.UCMTautofarm = false

function UCMTautofarm()
	while _G.UCMTautofarm == true do
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(652.0986938476562, 120.48353576660156, 2725.77880859375)
		wait()
	end
end

