--Special
local Section = Special:CreateSection("Special",true)
 local Toggle = Special:CreateToggle({
	Name = "Hide Name",
	CurrentValue = false,
	Flag = "Toggle1", -- A flag is the identifier for the configuration file, make sure every element has a different flag if you're using configuration saving to ensure no overlaps
	Callback = function(Value)
	if _G.name == Value then
		game:GetService("Players").LocalPlayer:WaitForChild("PlayerGui").BetterHealthBar:FindFirstChild("HealthGui").HealthFrame:FindFirstChild("Name").Visible=true
		game:GetService("Players").LocalPlayer.Character:WaitForChild("Head").BillboardGui:FindFirstChild("Name").Visible=true
    else
		game:GetService("Players").LocalPlayer:WaitForChild("PlayerGui").BetterHealthBar:FindFirstChild("HealthGui").HealthFrame:FindFirstChild("Name").Visible=false
		game:GetService("Players").LocalPlayer.Character:WaitForChild("Head").BillboardGui:FindFirstChild("Name").Visible=false
	end
	end,
 })

--Illegal
local Label = Illegal:CreateLabel("THIS HACK IS ILLEGAL")
local Button = Illegal:CreateButton({
	Name = "F R E E  A L L  C H A R A C T E R",
	Interact = 'Click',
	Callback = function()
	    for i, v in pairs(game:GetService("Workspace").CHARACTERSTUFF["CHARACTER BOX"]:GetDescendants()) do
			if v.Name == "Door" and v.Parent then
				v.Parent:Destroy()
			end
		end
	end,
 })

end
